<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title"></h3>
    </div>


    <div class="box-body">

        <div class="form-group">
            {{ form_label(form.dateMatch, "Date match ") }}
            {# Affichage des erreurs pour ce champ précis. #}
            {{ form_errors(form.dateMatch) }}
            {# Génération de l'input. #}
            {{ form_widget(form.dateMatch) }}
    
        </div>
        <div class="form-group">
            {{ form_label(form.heureDebut, "Heure de debut") }}
            {# Affichage des erreurs pour ce champ précis. #}
            {{ form_errors(form.heureDebut) }}
            {# Génération de l'input. #}
             {{ form_widget(form.heureDebut) }}
           
        </div>
             
                <div class="form-group">
            {{ form_label(form.heureFin, "Heure de Fin") }}
            {# Affichage des erreurs pour ce champ précis. #}
            {{ form_errors(form.heureFin) }}
            {# Génération de l'input. #}
             {{ form_widget(form.heureFin) }}
           
        </div>
             
         <div class="form-group">
            {{ form_label(form.equipes, "Choisit deux equipes") }}
            {# Affichage des erreurs pour ce champ précis. #}
            {{ form_errors(form.equipes) }}
            {# Génération de l'input. #}
             {{ form_widget(form.equipes) }}
           
        </div>
            
            
            
          <div class="box-footer">
        {{ form_widget(form.save, {'attr': {'class': 'btn btn-primary'}}) }}
    </div>

    </div>


</div><!-- box box-primary-->